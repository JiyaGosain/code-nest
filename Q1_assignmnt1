 //Q1 WAP to check whether a given is Armstrong or not
#include<stdio.h>
int main()
{
	int a,sum=0,r,n;
	printf("Enter value for a");
	scanf("%d",&a);
	n=a;
	while(a>0)
	{
		r=a%10;
		r=r*r*r;
		sum=sum+r;
		a=a/10;
	}
	if(sum==n)
	{
		printf("No is Armstrong");
	}
	else
	{
		printf("No is not Armstrong");
	}
}
